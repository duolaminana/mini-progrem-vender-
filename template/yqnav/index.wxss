/* 邀请 */
.tpl-yqnav-index,.tpl-yqnav-user{
    position: absolute;
    z-index: calc(var(--zindex_hd) - 1);
}
.tpl-yqnav-index{
    top: 42rpx;
    left: 72rpx;
}
.tpl-yqnav-index .tpl-yqnav-index-con,.tpl-yqnav-user .tpl-yqnav-user-con{
    font-size: 30rpx;
    font-weight: bold;
    color: var(--wc);
}
.tpl-yqnav-index .tpl-yqnav-index-btn,.tpl-yqnav-user .tpl-yqnav-user-btn{
    border: none;
    outline: none;
    margin: 0;
    display: block;
    width: 120rpx;
    height: 33rpx;
    line-height: 33rpx;
    margin-top: 34rpx;
    background-color: var(--wc);
    border-radius: 60rpx;
    text-align: center;
    font-size: 20rpx;
    font-weight: bold;
    color: #ED4014;
	padding: 0;
}

.tpl-yqnav-user{
    top: 28rpx;
    left: 38rpx;
}
.tpl-yqnav-user .tpl-yqnav-user-con{
    margin-top: 15rpx;
}
.tpl-yqnav-user .tpl-yqnav-user-btn{
    margin-top: 0;
}