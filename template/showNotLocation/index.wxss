.showNotLocation{
    --img-top: 38rpx;
    width: 100%;
    height: 256rpx;
    background-color: #f5f5f5;
    padding-top: calc(var(--paddingTop,0) - var(--img-top));
}
.showNotLocation-pic{
    width: 118rpx;
    height: 128rpx;
    margin: 0 auto;
    padding: var(--img-top) 0 27rpx;
}
.showNotLocation-pic image{
    width: 118rpx;
    height: 128rpx;
}
.showNotLocation-text{
    text-align: center;
    font-weight: var(--w4);
    font-size: 24rpx;
    color: var(--zc9);
}