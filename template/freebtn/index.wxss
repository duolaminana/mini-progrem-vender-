/* 悬浮按钮 扫码，去地图*/
.tpl-freebtn{
    width: 100%;
    height:108rpx;
}
.tpl-freebtn .tpl-freebtn-con{
    display: flex;
    align-items:center; /*垂直居中*/
    justify-content: center; /*水平居中*/
    height:88rpx;
    background-color:var(--zc3);
    border-radius:8rpx;
    position: fixed;
    bottom: 20rpx;
    left: 32rpx;
}
.tpl-freebtn .tpl-freebtn-con .tpl-freebtn-text{
    display: inline-block;
    font-size: 30rpx;
    font-weight: Medium;
    color: var(--wc);
    line-height: 0;
}
.tpl-freebtn .tpl-freebtn-con .tpl-freebtn-icon{
    display: inline-block;
    width: 30rpx;
    padding-right: 16rpx;
    margin-top: .5rpx;
}