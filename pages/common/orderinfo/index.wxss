.tpl_order_info{
    width: 100%;
    padding-top: 24rpx;
    padding-bottom: 24rpx;
}
.tpl_order_info .li{
    overflow: hidden;
    margin-bottom: 24rpx;
}
.tpl_order_info .li:nth-last-child(1){
    margin-bottom: 0;
}
.tpl_order_info .ll{
    float: left;
    font-weight: var(--w5);
    font-size: 24rpx;
    color: var(--zc9);
}
.tpl_order_info .rr{
    float: right;
    font-family: var(--fa);
    font-weight: var(--w4);
    font-size: 24rpx;
    color: var(--zc9);
}