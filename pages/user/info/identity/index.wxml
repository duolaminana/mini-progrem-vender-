<!--pages/user/info/identity/identity.wxml-->
<import src="/template/head/head.wxml"></import>
<import src="/template/freebtn/common.wxml"></import>

<view class="container cus-lan" style="--height--:{{__hdHeight__}};">
    <template is="tpl-header" data="{{...hdData}}"></template>

    <view class="user-info">
        <view class="li">
            <view class="text">真实姓名</view>
            <view class="note">
                <input type="text" placeholder="请输入真实姓名"/>
            </view>
        </view>
        <view class="li">
            <view class="text">身份证号</view>
            <view class="note">
                <input type="number" placeholder="请输入身份证号"/>
            </view>
        </view>
    </view>

    <template is="tpl-freebtn" data="{{...fbData}}"></template>
</view>
